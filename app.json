{
  "expo": {
    "name": "MultitanquesSAS",
    "slug": "MultitanquesSAS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Multitanques.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fabianbeltran.SingApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Multitanques.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "fabianbeltran.SingApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "37f507aa-e6b2-4008-8ed8-f86caa2bb674"
      }
    }
  }
}
